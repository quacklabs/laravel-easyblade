{
    "name": "rezaamini-ir/laravel-easyblade",
    "license": "MIT",
    "description": "Create a easier & readable Blade view with EasyBlade",
    "authors": [
        {
            "name": "Reza Amini",
            "email": "rezaaminiroyal@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "EasyBlade\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BladeTest\\" : "tests/"
        }
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "EasyBlade\\EasyBladeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
